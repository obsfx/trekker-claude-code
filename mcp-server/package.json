{
  "name": "@obsfx/trekker-mcp",
  "version": "1.7.6",
  "description": "MCP server for trekker issue tracker",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "trekker-mcp": "dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "esbuild src/index.ts --bundle --platform=node --target=node18 --format=esm --outfile=dist/index.js",
    "build:tsc": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/index.js",
    "prepublishOnly": "npm run build",
    "version:patch": "npm version patch --no-git-tag-version",
    "version:minor": "npm version minor --no-git-tag-version",
    "version:major": "npm version major --no-git-tag-version"
  },
  "keywords": [
    "mcp",
    "trekker",
    "issue-tracker",
    "ai-workflow",
    "claude",
    "anthropic"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/obsfx/trekker-claude-code.git",
    "directory": "mcp-server"
  },
  "bugs": {
    "url": "https://github.com/obsfx/trekker-claude-code/issues"
  },
  "homepage": "https://github.com/obsfx/trekker-claude-code#readme",
  "author": "Omer Can Balandi",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0",
    "zod": "^3.25.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "esbuild": "^0.27.2",
    "typescript": "^5.7.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
